<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">

<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>Updates - brew &amp; conda packages</string>
        <key>ProgramArguments</key>
        <array>
            <string>/bin/bash</string>
            <string>~/.automation/update.sh</string>
        </array>
        <key>WorkingDirectory</key>
        <string/>
        <key>StartCalendarInterval</key>
        <dict>
            <key>Weekday</key>
            <integer>7</integer>
            <key>Hour</key>
            <integer>1</integer>
            <key>Minute</key>
            <integer>0</integer>
        </dict>
    </dict>
</plist>